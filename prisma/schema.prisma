generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())

  username     String @unique
  email        String @unique
  hashPassword String @map("hash_password")

  updateAt DateTime @updatedAt @map("update_at")
}
